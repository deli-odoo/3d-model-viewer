<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="product_template_product_form_3d_model_viewer_inherit" model="ir.ui.view">
        <field name="name">product.template.product.form.3d.model.viewer.inherit</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_only_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook" position="inside">
                <page string="3D Model" name="3d_model">
                    <field name="model_viewer" filename="file_name" widget="model_viewer" />
                    <field name="file_name" invisible="1"/>
                </page>
            </xpath>
        </field>
    </record>
</odoo>